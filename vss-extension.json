{
  "manifestVersion": 1,
  "publisher": "ReversingLabs",
  "id": "rl-scanner-cloud-task",
  "version": "1.0.6",
  "name": "ReversingLabs rl-scanner-cloud task",
  "description": "ReversingLabs rl-scanner-cloud as a Azure DevOps extension",
  "public": true,
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "categories": [
    "Azure Pipelines"
  ],
  "icons": {
    "default": "images/reversinglabs_logo.jpeg"
  },
  "files": [
    {
      "path": "scan-cloud-task"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    },
    "license": {
      "path": "eula.md"
    }
  },
  "links": {
    "home": {
      "uri": "https://www.reversinglabs.com/"
    },
    "getstarted": {
      "uri": "https://docs.secure.software/portal/"
    },
    "learn": {
      "uri": "https://docs.secure.software/portal/integrations"
    },
    "support": {
      "uri": "https://www.reversinglabs.com/contact-us"
    },
    "repository": {
      "uri": "https://github.com/reversinglabs/rl-scanner-cloud-ado-extension"
    },
    "issues": {
      "uri": "https://github.com/reversinglabs/rl-scanner-cloud-ado-extension/issues"
    }
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/reversinglabs/rl-scanner-cloud-ado-extension"
  },
  "tags": [
    "ReversingLabs",
    "Spectra Assure",
    "Secure Software",
    "Software Supply Chain"
  ],
  "contributions": [
    {
      "id": "custom-build-release-task",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "scan-cloud-task"
      }
    }
  ]
}
